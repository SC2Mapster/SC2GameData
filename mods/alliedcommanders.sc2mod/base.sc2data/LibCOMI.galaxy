    TriggerQueueEnter();
    libCOMI_gf_CM_CommanderVOSend(libCOOC_gf_CC_PlayerAlly(libCOOC_gf_CC_CommanderPlayer("ProtossKarax")), SoundLink("KaraxCommander_ChronoPassive", -1), PlayerGroupAll());
    TriggerQueueExit();
    return true;
}

