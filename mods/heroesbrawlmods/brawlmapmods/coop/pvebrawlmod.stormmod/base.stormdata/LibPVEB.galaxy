    libPVEB_gv_pvEResurrectTimes[lv_casterPlayer] += 1;
    libGame_gf_ScoreValuesReportIntegerValueAndLog(lv_casterPlayer, "Resurrect", libPVEB_gv_pvEResurrectTimes[lv_casterPlayer]);
