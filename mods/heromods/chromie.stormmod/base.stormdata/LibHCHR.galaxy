//--------------------------------------------------------------------------------------------------
// Trigger: Hero - Chromie - Timewalker's Pursuit Proc
//--------------------------------------------------------------------------------------------------
bool libHCHR_gt_HeroChromieTimewalkersPursuitProc_Func (bool testConds, bool runActions) {
    // Variable Declarations
    int lv_player;
    unit lv_unit;

    // Automatic Variable Declarations
    // Variable Initialization
    lv_player = EventPlayerEffectUsedUnitOwner(c_effectPlayerTarget);
    lv_unit = EventPlayerEffectUsedUnit(c_effectUnitTarget);

    // Conditions
    if (testConds) {
        if (!((lv_player <= libCore_gv_bALMaxPlayers))) {
            return false;
        }

        if (!((lv_player > 0))) {
            return false;
        }

        if (!((PlayerHasTalent(lv_player, "ChromieTimewalkersPursuit") == true))) {
            return false;
        }

        if (!((UnitGetType(lv_unit) == "HeroChromie"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    UnitCreateEffectUnit(lv_unit, "ChromieTimewalkersPursuitModifyQuestToken", lv_unit);
    return true;
}

