    gv_raynorWaveBoost = true;
    while ((libNtve_gf_UnitGroupIsDead(lv_dropships) == false) && (libVCMI_gf_GameIsOver() == false)) {
        UnitGroupLoopBegin(lv_dropships);
        for ( ; !UnitGroupLoopDone() ; UnitGroupLoopStep() ) {
            if ((libNtve_gf_UnitInRegion(UnitGroupLoopCurrent(), RegionFromId(10)) == true)) {
                UnitRemove(UnitGroupLoopCurrent());
            }
            else {
                UnitIssueOrder(UnitGroupLoopCurrent(), OrderTargetingPoint(AbilityCommand("move", 0), RegionGetCenter(RegionFromId(10))), c_orderQueueReplace);
            }
        }
        UnitGroupLoopEnd();
        Wait(1.0, c_timeAI);
    }
    return true;
}

