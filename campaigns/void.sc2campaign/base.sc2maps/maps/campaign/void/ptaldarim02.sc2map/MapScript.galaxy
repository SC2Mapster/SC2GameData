//--------------------------------------------------------------------------------------------------
// Trigger: Tal'Darim Pylon Dies
//--------------------------------------------------------------------------------------------------
bool gt_TalDarimPylonDies_Func (bool testConds, bool runActions) {
    // Conditions
    if (testConds) {
        if (!(((UnitGetOwner(EventUnit()) == gv_pLAYER_02_TALDARIM) || (UnitGetOwner(EventUnit()) == gv_pLAYER_03_TALDARIM) || (UnitGetOwner(EventUnit()) == gv_pLAYER_04_TALDARIM)))) {
            return false;
        }

        if (!((UnitGetType(EventUnit()) == "Pylon"))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    UnitGroupLoopBegin(UnitGroup("InvisiblePylon", gv_pLAYER_02_TALDARIM, RegionCircle(UnitGetPosition(EventUnit()), 1.0), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0));
    for ( ; !UnitGroupLoopDone() ; UnitGroupLoopStep() ) {
        UnitRemove(UnitGroupLoopCurrent());
    }
    UnitGroupLoopEnd();
    UnitGroupLoopBegin(UnitGroup("InvisiblePylon", gv_pLAYER_05_TALDARIM, RegionCircle(UnitGetPosition(EventUnit()), 1.0), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0));
    for ( ; !UnitGroupLoopDone() ; UnitGroupLoopStep() ) {
        UnitRemove(UnitGroupLoopCurrent());
    }
    UnitGroupLoopEnd();
    return true;
}

