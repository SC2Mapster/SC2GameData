bool gt_IntroAlarakMalashAttackLogic_Func (bool testConds, bool runActions) {
    int auto275A930E_val;

    // Conditions
    if (testConds) {
        if (!((TriggerIsEnabled(TriggerGetCurrent()) == true))) {
