    if (!runActions) {
        return true;
    }

    UnitGroupLoopBegin(UnitGroup("Pylon", c_playerAny, RegionPlayableMap(), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0));
