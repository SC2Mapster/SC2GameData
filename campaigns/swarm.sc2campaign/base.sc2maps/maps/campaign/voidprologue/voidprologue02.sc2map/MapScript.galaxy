    else {
    }
    UnitGroupLoopBegin(UnitGroup("PrologueFissureBreach", c_playerAny, RegionFromId(17), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32))), 0));
    for ( ; !UnitGroupLoopDone() ; UnitGroupLoopStep() ) {
        ActorSend(libHots_gf_ActorForUnit(UnitGroupLoopCurrent(), 1), "Signal Work");
