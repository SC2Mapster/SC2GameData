        AIAttackWaveSend(gv_p4_PROTOSS_WEST_ESCORTS, 0, false);
        gv_terrazineAttackWaves[FixedToInt(UnitGetCustomValue(EventUnitTargetUnit(), 0))][1] = WaveLastCreated();
