<?xml version="1.0" encoding="us-ascii"?>
<Catalog>
    <CReward default="1">
        <Name value="Reward/Name/##id##"/>
    </CReward>
    <CRewardDecal default="1">
        <?token id="iconfile" type="CImagePath"?>
        <Texture value="##id##"/>
        <IconFile value="##file##"/>
        <IconCols value="8"/>
        <IconRows value="8"/>
    </CRewardDecal>
    <CRewardIcon default="1">
        <?token id="iconfile" type="CImagePath"?>
        <IconFile value="##file##"/>
        <IconCols value="9"/>
        <IconRows value="9"/>
    </CRewardIcon>
    <CRewardModel default="1">
        <?token id="iconfile" type="CImagePath"?>
        <Model value="##id##"/>
        <!-- Intentionally not set since Battle.net does not expect them. -->
        <!--<IconFile value="##file##"/>-->
        <IconCols value="6"/>
        <IconRows value="6"/>
    </CRewardModel>
    <CRewardPortrait default="1">
        <?token id="iconfile" type="CImagePath"?>
        <Model value="##id##"/>
        <IconFile value="##file##"/>
        <IconCols value="6"/>
        <IconRows value="6"/>
    </CRewardPortrait>
</Catalog>
