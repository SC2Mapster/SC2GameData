//--------------------------------------------------------------------------------------------------
//  TerranOpenInitGnd4
//--------------------------------------------------------------------------------------------------
static void TerranOpenInitGnd4(int player) {
    AISetDifficulty(player, c_diffEarlyGameRepair, true);

    AINewChooseSubState(player, e_openRollState, 1, 1000, e_mainSubState_OpenMarineMix4);

    // Terran quick expand will only work if we wall in and expand to an
    //  air location, so not using this for now.
    //if (AIHasNearbyOpenExpansion(player)) {
    //    AISetSubStateChance( e_mainSubState_OpenQuickExpandGnd4,  5 );
    //}

    if (AIGetNumEnemies(player) == 1) {
        // single enemy
        if (AIGetAirStartLocs(PlayerStartLocation(player)) == 0) {
            // all start locations are accessible on the ground      random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_OpenPureMarine4,    4,     2,     6,     4 ); //   4,   2,   6,   4 
            AISetSubStateChanceRace( e_mainSubState_OpenReaperRush4,    4,     4,     2,     4 ); //   8,   6,   8,   8
            AISetSubStateChanceRace( e_mainSubState_OpenMarineMix4,    90,    90,    88,    90 ); //  98,  96,  96,  98
            AISetSubStateChanceRace( e_mainSubState_OpenTechBoom4,      2,     4,     4,     2 ); // 100, 100, 100, 100
        }
        else {
            // might not be able to reach them on the ground         random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_OpenPureMarine4,    2,     2,     2,     2 ); //   2,   2,   2,   2
            AISetSubStateChanceRace( e_mainSubState_OpenReaperRush4,    2,     2,     2,     2 ); //   4,   4,   4,   4
            AISetSubStateChanceRace( e_mainSubState_OpenMarineMix4,    86,    84,    84,    88 ); //  90,  88,  88,  92
            AISetSubStateChanceRace( e_mainSubState_OpenTechBoom4,     10,    12,    12,     8 ); // 100, 100, 100, 100
        }
    }
    else {
        // multiple enemies
        if (AIGetAirStartLocs(PlayerStartLocation(player)) == 0) {
            // all start locations are accessible on the ground
            AISetSubStateChance( e_mainSubState_OpenPureMarine4,    4 ); //   4
            AISetSubStateChance( e_mainSubState_OpenReaperRush4,    2 ); //   6 
            AISetSubStateChance( e_mainSubState_OpenMarineMix4,    90 ); //  96 
            AISetSubStateChance( e_mainSubState_OpenTechBoom4,      4 ); // 100
        }
        else {
            // might not be able to reach them on the ground
            AISetSubStateChance( e_mainSubState_OpenPureMarine4,    2 ); //   2
            AISetSubStateChance( e_mainSubState_OpenReaperRush4,    2 ); //   4 
            AISetSubStateChance( e_mainSubState_OpenMarineMix4,    84 ); //  88 
            AISetSubStateChance( e_mainSubState_OpenTechBoom4,     12 ); // 100
        }
    }

    AISetMainState(player, e_mainState_OpenGnd4, AIChooseSubState());
}

