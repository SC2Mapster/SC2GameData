//--------------------------------------------------------------------------------------------------
//  TerranMid4
//--------------------------------------------------------------------------------------------------
void TerranMid4 (int player) {
    int mainSubState = AIState(player, e_mainSubState);

    if (AIGetGroundStartLocs(PlayerStartLocation(player)) > 0) {
        AIScoutingEnable(player, 12, c_TU_SCV);
        AIEarlyDefenseScouting(player);
    }
    else {
        AIScoutingEnable(player, 1, c_TU_Medivac);
    }

    if (mainSubState == e_mainSubState_MidMarineRush4)            { TerranMidMarineRush4(player); }
    else if (mainSubState == e_mainSubState_MidMarineReaper4)     { TerranMidMarineReaper4(player); }
    else if (mainSubState == e_mainSubState_MidMarineMarauder4)   { TerranMidMarineMarauder4(player); }
    else if (mainSubState == e_mainSubState_MidMarauderMarine4)   { TerranMidMarauderMarine4(player); }
    else if (mainSubState == e_mainSubState_MidMMPlusMedivac4)    { TerranMidMMPlusMedivac4(player); }
    else if (mainSubState == e_mainSubState_MidMMPlusSiege4)      { TerranMidMMPlusSiege4(player); }
    else if (mainSubState == e_mainSubState_MidMMPlusThor4)       { TerranMidMMPlusThor4(player); }
    else if (mainSubState == e_mainSubState_MidReaperRush4)       { TerranMidReaperRush4(player); }
    else if (mainSubState == e_mainSubState_MidBansheeRush4)      { TerranMidBansheeRush4(player); }
    else if (mainSubState == e_mainSubState_MidAirDropRush4)      { TerranMidAirDropRush4(player); }
    else if (mainSubState == e_mainSubState_MidAirFlierRush4)     { TerranMidAirFlierRush4(player); }
    else if (mainSubState == e_mainSubState_MidAirDrop4)          { TerranMidAirDrop4(player); }
    else if (mainSubState == e_mainSubState_MidAirViking4)        { TerranMidAirViking4(player); }
    else if (mainSubState == e_mainSubState_MidAirCloakBanshee4)  { TerranMidAirCloakBanshee4(player); }
    else if (mainSubState == e_mainSubState_MidAirBattlecruiser4) { TerranMidAirBattlecruiser4(player); }
    else { ErrorMeleeScript(player, "Invalid Mid mainSubState"); }
}

