//--------------------------------------------------------------------------------------------------
//  IsSubStateStillValid
//--------------------------------------------------------------------------------------------------
static bool IsSubStateStillValid (int player, int subState) {
    if (subState == e_mainSubState_LateMMMediViking4)            { TerranLateMMMediViking4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateMMMediGhost4)        { TerranLateMMMediGhost4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateMMMediSeiege4)       { TerranLateMMMediSeiege4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateMMMediThor4)         { TerranLateMMMediThor4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateMMMediBattle4)       { TerranLateMMMediBattle4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateMMMediHarass4)       { TerranLateMMMediHarass4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateSiegeThor4)          { TerranLateSiegeThor4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateVikingBattle4)       { TerranLateVikingBattle4(player, e_armyTypeValidate); }
    else if (subState == e_mainSubState_LateVikingRavenBattle4)  { TerranLateVikingRavenBattle4(player, e_armyTypeValidate); }
    else { ErrorMeleeScript(player, "Invalid Late SubState in IsSubStateStillValid??"); }
    return false;
}

//--------------------------------------------------------------------------------------------------
//  TerranChooseNextLateGameArmy4
//--------------------------------------------------------------------------------------------------
static void TerranChooseNextLateGameArmy4 (int player, int state) {
    int subState;
    int subStateTime;
    bool stillValid;

    subState = AIGetUserInt(player, c_lateGameStateIndex);
    if (subState >= e_mainSubState_LateMMMediViking4) {
        if (subState <= e_mainSubState_LateVikingRavenBattle4) {
            subStateTime = AIGetUserInt(player, c_lateGameTimeIndex);
            if (subStateTime > AIGetTime()) {
                stillValid = IsSubStateStillValid(player, subState);
                if (stillValid) {
                    return;  // keep using the previous state
                }
            }
        }
    }

    // need to choose a new late game state
    
    AINewChooseSubState(player, e_lateRollState, 1, 1000, e_mainSubState_LateMMMediViking4);

    if (AIGetNumEnemies(player) == 1) {
        if (AIGetAirStartLocs(PlayerStartLocation(player)) == 0) {
            // all start locations are accessible on the ground           random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_LateMMMediViking4,      16,    20,    16,    16 ); //  16,  20,  16,  16
            AISetSubStateChanceRace( e_mainSubState_LateMMMediGhost4,       20,    30,    12,    12 ); //  36,  50,  28,  28
            AISetSubStateChanceRace( e_mainSubState_LateMMMediSeiege4,      16,    10,    22,    20 ); //  52,  60,  50,  48
            AISetSubStateChanceRace( e_mainSubState_LateMMMediThor4,        12,     8,    12,    12 ); //  64,  68,  62,  60
            AISetSubStateChanceRace( e_mainSubState_LateMMMediBattle4,       4,     6,     4,     4 ); //  68,  74,  66,  64
            AISetSubStateChanceRace( e_mainSubState_LateMMMediHarass4,      12,    12,    12,    12 ); //  80,  86,  78,  76
            AISetSubStateChanceRace( e_mainSubState_LateSiegeThor4,         10,     4,    12,    12 ); //  90,  90,  90,  88
            AISetSubStateChanceRace( e_mainSubState_LateVikingBattle4,       4,     4,     4,     4 ); //  94,  94,  94,  92
            AISetSubStateChanceRace( e_mainSubState_LateVikingRavenBattle4,  6,     6,     6,     8 ); // 100, 100, 100, 100
        }
        else if (AIGetGroundStartLocs(PlayerStartLocation(player)) == 0) {
            // no start locations are accesible on the ground             random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_LateMMMediViking4,      12,    12,    12,    12 ); //  12,  12,  12,  12
            AISetSubStateChanceRace( e_mainSubState_LateMMMediGhost4,       10,    16,     8,     6 ); //  22,  28,  20,  18
            AISetSubStateChanceRace( e_mainSubState_LateMMMediSeiege4,      12,     8,    16,    14 ); //  34,  36,  36,  32
            AISetSubStateChanceRace( e_mainSubState_LateMMMediThor4,         8,     8,     8,     6 ); //  42,  44,  44,  38
            AISetSubStateChanceRace( e_mainSubState_LateMMMediBattle4,       6,     6,     4,     4 ); //  48,  50,  48,  42
            AISetSubStateChanceRace( e_mainSubState_LateMMMediHarass4,      10,     8,    10,    10 ); //  58,  58,  58,  52
            AISetSubStateChanceRace( e_mainSubState_LateSiegeThor4,          2,     2,     2,     2 ); //  60,  60,  60,  54
            AISetSubStateChanceRace( e_mainSubState_LateVikingBattle4,      16,    16,    16,    16 ); //  76,  76,  76,  70
            AISetSubStateChanceRace( e_mainSubState_LateVikingRavenBattle4, 24,    24,    24,    30 ); // 100, 100, 100, 100
        }
        else {
            // may or may not be able to reach them on the ground         random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_LateMMMediViking4,      14,    16,    14,    12 ); //  14,  16,  14,  12
            AISetSubStateChanceRace( e_mainSubState_LateMMMediGhost4,       18,    22,    10,    10 ); //  32,  38,  24,  22
            AISetSubStateChanceRace( e_mainSubState_LateMMMediSeiege4,      12,     8,    18,    16 ); //  44,  46,  42,  38
            AISetSubStateChanceRace( e_mainSubState_LateMMMediThor4,        10,     8,    10,     8 ); //  54,  54,  52,  46
            AISetSubStateChanceRace( e_mainSubState_LateMMMediBattle4,       4,     4,     4,     4 ); //  58,  58,  56,  50
            AISetSubStateChanceRace( e_mainSubState_LateMMMediHarass4,      10,    10,    10,    10 ); //  68,  68,  66,  60
            AISetSubStateChanceRace( e_mainSubState_LateSiegeThor4,          4,     4,     6,     4 ); //  72,  72,  72,  64
            AISetSubStateChanceRace( e_mainSubState_LateVikingBattle4,      12,    12,    12,    12 ); //  84,  84,  84,  76
            AISetSubStateChanceRace( e_mainSubState_LateVikingRavenBattle4, 16,    16,    16,    24 ); // 100, 100, 100, 100
        }
    }
    else {
        // originally these multiplayer values were going to be different, so I'm leaving these seperate for now
        if (AIGetAirStartLocs(PlayerStartLocation(player)) == 0) {
            // all start locations are accessible on the ground 
            AISetSubStateChance( e_mainSubState_LateMMMediViking4,      16); //  16
            AISetSubStateChance( e_mainSubState_LateMMMediGhost4,       20); //  36
            AISetSubStateChance( e_mainSubState_LateMMMediSeiege4,      16); //  52
            AISetSubStateChance( e_mainSubState_LateMMMediThor4,        12); //  64
            AISetSubStateChance( e_mainSubState_LateMMMediBattle4,       4); //  68
            AISetSubStateChance( e_mainSubState_LateMMMediHarass4,      12); //  80
            AISetSubStateChance( e_mainSubState_LateSiegeThor4,         10); //  90
            AISetSubStateChance( e_mainSubState_LateVikingBattle4,       4); //  94
            AISetSubStateChance( e_mainSubState_LateVikingRavenBattle4,  6); // 100
        }        
        else if (AIGetGroundStartLocs(PlayerStartLocation(player)) == 0) {
            // no start locations are accesible on the ground
            AISetSubStateChance( e_mainSubState_LateMMMediViking4,      12); //  12
            AISetSubStateChance( e_mainSubState_LateMMMediGhost4,       10); //  22
            AISetSubStateChance( e_mainSubState_LateMMMediSeiege4,      12); //  34
            AISetSubStateChance( e_mainSubState_LateMMMediThor4,         8); //  42
            AISetSubStateChance( e_mainSubState_LateMMMediBattle4,       6); //  48
            AISetSubStateChance( e_mainSubState_LateMMMediHarass4,      10); //  58
            AISetSubStateChance( e_mainSubState_LateSiegeThor4,          2); //  60
            AISetSubStateChance( e_mainSubState_LateVikingBattle4,      16); //  76
            AISetSubStateChance( e_mainSubState_LateVikingRavenBattle4, 24); // 100
        }
        else {
            // may or may not be able to reach them on the ground
            AISetSubStateChance( e_mainSubState_LateMMMediViking4,      14); //  14
            AISetSubStateChance( e_mainSubState_LateMMMediGhost4,       18); //  32
            AISetSubStateChance( e_mainSubState_LateMMMediSeiege4,      12); //  44
            AISetSubStateChance( e_mainSubState_LateMMMediThor4,        10); //  54
            AISetSubStateChance( e_mainSubState_LateMMMediBattle4,       4); //  58
            AISetSubStateChance( e_mainSubState_LateMMMediHarass4,      10); //  68
            AISetSubStateChance( e_mainSubState_LateSiegeThor4,          4); //  72
            AISetSubStateChance( e_mainSubState_LateVikingBattle4,      12); //  84
            AISetSubStateChance( e_mainSubState_LateVikingRavenBattle4, 16); // 100
        }
    }

    subState = AIChooseSubState();

    stillValid = IsSubStateStillValid(player, subState);
    if (!stillValid) {
        subState = e_mainSubState_LateMMMediViking4;   // this subState needs to always return valid
    }
    
    AISetUserInt(player, c_lateGameStateIndex, subState);
    AISetUserInt(player, c_lateGameTimeIndex, FixedToInt(AIGetTime()) + c_lateGameFrequency);
}

//--------------------------------------------------------------------------------------------------
//  TerranLateGameArmy4
//--------------------------------------------------------------------------------------------------
static void TerranLateGameArmy4 (int player, int state) {
    int subState;

    if (state == e_armyTypeChoose) {
        TerranChooseNextLateGameArmy4(player, state);
        return;
    }

    subState = AIGetUserInt(player, c_lateGameStateIndex);
    if (subState == e_mainSubState_LateMMMediViking4)            { TerranLateMMMediViking4(player, state); }
    else if (subState == e_mainSubState_LateMMMediGhost4)        { TerranLateMMMediGhost4(player, state); }
    else if (subState == e_mainSubState_LateMMMediSeiege4)       { TerranLateMMMediSeiege4(player, state); }
    else if (subState == e_mainSubState_LateMMMediThor4)         { TerranLateMMMediThor4(player, state); }
    else if (subState == e_mainSubState_LateMMMediBattle4)       { TerranLateMMMediBattle4(player, state); }
    else if (subState == e_mainSubState_LateMMMediHarass4)       { TerranLateMMMediHarass4(player, state); }
    else if (subState == e_mainSubState_LateSiegeThor4)          { TerranLateSiegeThor4(player, state); }
    else if (subState == e_mainSubState_LateVikingBattle4)       { TerranLateVikingBattle4(player, state); }
    else if (subState == e_mainSubState_LateVikingRavenBattle4)  { TerranLateVikingRavenBattle4(player, state); }
    else { ErrorMeleeScript(player, "Invalid Late SubState"); }
}

//--------------------------------------------------------------------------------------------------
//  TerranHarass4
//--------------------------------------------------------------------------------------------------
void TerranHarass4 (int player) {

    // Nuke harrassment
    if (AITechCount(player, c_TU_Ghost, c_techCountCompleteOnly) >= 2) {
        if (AITechCount(player, c_TB_GhostAcademy, c_techCountCompleteOnly) >= 1) {
            AISetStock( player, 1, c_TR_GhostCloak );
            if (AITechCount(player, c_TR_GhostCloak, c_techCountCompleteOnly) >= 1) {
                AISetStock( player, 1, c_TU_Nuke );
            }
        }
    }
}


//--------------------------------------------------------------------------------------------------
//  TerranLate4
//--------------------------------------------------------------------------------------------------
void TerranLate4 (int player) {
    // only allow scouting if I have an viking to scout with, ie stop using workers
    AIScoutingEnable(player, 1, c_TU_Medivac);
    // calculate a limit on how much simultaneous teching can happen
    AISetTechLimitLevels(player);


// ****************** Start Restock ****************** //
    AIClearStock(player);

// Very basic recovery start
    TerranRecoveryStart4(player);

// Very basic farms & peons
    AISetStockFarms( player, c_TB_SupplyDepot, c_stockNormalFarms);
    AISetStockPeons( player, 16, c_TU_SCV, c_stockMinPeons, c_stockAlways);

// Desperate expand (we're about to run out of minerals/gas)
    AIDefaultExpansion(player, c_TB_CommandCenter, 3000, 1000, c_expandVitalHard);

// Choose/validate what type of endgame army we'd like to make
    TerranLateGameArmy4(player, e_armyTypeChoose);

// Bare minimum army
    TerranLateGameArmy4(player, e_armyTypeMinimum);

// Basic peons
    AISetStockPeons( player, 24, c_TU_SCV, c_stockMinPeons, c_stockAlways);

// Urgent early detection
    if (AIGetCloakedAttacker(player) != null) {
        AISetStock( player, 1, c_TB_OrbitalCommand );
    }

// Barely adequete army including anti-air
    TerranLateGameArmy4(player, e_armyTypeBase);
    
// Basic towns, farms, & peons
    AIDefaultEconomy(player, c_TB_CommandCenter_Alias, c_TB_Refinery, c_TB_SupplyDepot, c_TU_SCV, 50, c_stockAlways);

// Low expand (we're running low on minerals/gas)
    AIDefaultExpansion(player, c_TB_CommandCenter, 5000, 2500, c_expandVitalHard);

// Minimum normal detection
    if (AISuspectDetectionDanger(player)) {
        AISetStock( player, 1, c_TB_OrbitalCommand );
    }

// Basic small army
    TerranLateGameArmy4(player, e_armyTypeSmall);
    
// Very important tech & a scout
    AISetStock( player, 1, c_TU_Medivac ); 

// Basic production buildings
    AISetStock( player, 2, c_TB_Barracks );
    AISetStock( player, 1, c_TB_BarracksTechLab );
    AISetStock( player, 1, c_TB_Factory );
    AISetStock( player, 1, c_TB_FactoryTechLab );

// Make Counter Units
    TerranMakeCounters4(player);
    
// Medium sized army
    TerranLateGameArmy4(player, e_armyTypeMedium);

// Normal expand
    AIDefaultExpansion(player, c_TB_CommandCenter, 12000, 6000, c_expandNormalHard);

// Multiple detectors & scouts & a transport
    AISetStockUnitNext( player, 2, c_TU_Medivac, c_stockAlways );
    AISetStockUnitNext( player, 2, c_TU_Raven, c_stockAlways );

// Important upgrades
    TerranImportantUpgrades4(player);

// Main base defense
    TerranDefenseMainTown4(player);

// Addition production buildings
    AISetStockUnitNext( player, 3, c_TB_Barracks,           c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_BarracksTechLab,    c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_BarracksReactor,    c_stockAlways );
    AISetStockUnitNext( player, 2, c_TB_BarracksTechLab,    c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_Factory,            c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_FactoryTechLab,     c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_Starport,           c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_StarportTechLab,    c_stockAlways );

// Multiple transports if needed
    if (AIWantsMultipleTransport(player)) {
        AISetStockUnitNext( player, 5, c_TU_Medivac, c_stockAlways );
    }

// Terran harrassment
    TerranHarass4(player);

// Large sized army
    TerranLateGameArmy4(player, e_armyTypeLarge);

// Other towns defense
    TerranDefenseOtherTowns4(player);

// Minor upgrades
    TerranMinorUpgrades4(player);
    
// Extra detection & scouts & transports
    AISetStockUnitNext( player, 4, c_TU_Raven, c_stockAlways );
    AISetStockUnitNext( player, 6, c_TU_Medivac, c_stockAlways );
  
// Extra production buildings
    AISetStockUnitNext( player, 4, c_TB_Barracks,           c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_BarracksTechLab,    c_stockAlways );
    AISetStockUnitNext( player, 1, c_TB_BarracksReactor,    c_stockAlways );
    AISetStockUnitNext( player, 3, c_TB_BarracksTechLab,    c_stockAlways );
    AISetStockUnitNext( player, 3, c_TB_Factory,            c_stockAlways );
    AISetStockUnitNext( player, 3, c_TB_FactoryTechLab,     c_stockAlways );
    AISetStockUnitNext( player, 3, c_TB_Starport,           c_stockAlways );
    AISetStockUnitNext( player, 3, c_TB_StarportTechLab,    c_stockAlways );

// Extra research buildings
    if (AIHasRes(player,650,500)) {
        AISetStockUnitNext( player, 2, c_TB_EngineeringBay, c_stockAlways );
        AISetStockUnitNext( player, 2, c_TB_Armory, c_stockAlways );
    }

// Entire Army
    TerranLateGameArmy4(player, e_armyTypeFull);

// Idle expand (very low priority, to prevent us from sitting around with idle peons)
    AIDefaultExpansion(player, c_TB_CommandCenter, 24000, 12000, c_expandIdleHard);

    AIEnableStock(player);    
// ****************** End Restock ****************** //

}
