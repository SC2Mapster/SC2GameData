            AISetSubStateChanceRace( e_mainSubState_MidMarineReaper4,     2,     2,     2,     2 );  //   6,   6,   6,   6
            AISetSubStateChanceRace( e_mainSubState_MidMarineMarauder4,  22,    26,    24,    18 );  //  28,  32,  30,  24
