//--------------------------------------------------------------------------------------------------
//  ZergOpenInitGnd4
//--------------------------------------------------------------------------------------------------
static void ZergOpenInitGnd4(int player) {
    int race = AIGetEnemyRaceVal(player);

    AILimitStockLarva(player, c_ZU_Larva);

    AINewChooseSubState(player, e_openRollState, 1, 1000, e_mainSubState_OpenBasicRoach4);

    if (race != c_raceMulti) {
        // single enemy
        if (AIGetAirStartLocs(PlayerStartLocation(player)) == 0) {
            if (race == c_raceTerran && AIHasNearbyOpenExpansion(player)) {
                AISetSubStateChance( e_mainSubState_OpenQuickExpandGnd4,  4 );
            }

            // all start locations are accessible on the ground       random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_Open6Pool4,          2,     4,     2,     2 ); //   2,   4,   2,   2
            AISetSubStateChanceRace( e_mainSubState_OpenBasicZergling4,  6,     6,     6,     2 ); //   8,  10,   8,   4
            AISetSubStateChanceRace( e_mainSubState_OpenBasicRoach4,    60,    56,    38,    62 ); //  68,  66,  46,  66
            AISetSubStateChanceRace( e_mainSubState_OpenRoachTech4,     30,    32,    52,    32 ); //  98,  98,  98,  98
            AISetSubStateChanceRace( e_mainSubState_OpenLingTech4,       2,     2,     2,     2 ); // 100, 100, 100, 100
        }
        else {
            if (race == c_raceTerran && AIHasNearbyOpenExpansion(player)) {
                AISetSubStateChance( e_mainSubState_OpenQuickExpandGnd4,  8 );
            }

            // might not be able to reach them on the ground          random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_Open6Pool4,          2,     2,     2,     2 ); //   2,   2,   2,   2
            AISetSubStateChanceRace( e_mainSubState_OpenBasicZergling4,  2,     2,     2,     2 ); //   4,   4,   4,   4
            AISetSubStateChanceRace( e_mainSubState_OpenBasicRoach4,    32,    32,    24,    30 ); //  36,  36,  28,  34
            AISetSubStateChanceRace( e_mainSubState_OpenRoachTech4,     62,    62,    70,    64 ); //  98,  98,  98,  98
            AISetSubStateChanceRace( e_mainSubState_OpenLingTech4,       2,     2,     2,     2 ); // 100, 100, 100, 100
        }
    }
    else {
        // multiple enemies
        if (AIGetAirStartLocs(PlayerStartLocation(player)) == 0) {
            // all start locations are accessible on the ground
            AISetSubStateChance( e_mainSubState_Open6Pool4,          2 ); //   2
            AISetSubStateChance( e_mainSubState_OpenBasicZergling4,  6 ); //   8
            AISetSubStateChance( e_mainSubState_OpenBasicRoach4,    56 ); //  64
            AISetSubStateChance( e_mainSubState_OpenRoachTech4,     34 ); //  98 
            AISetSubStateChance( e_mainSubState_OpenLingTech4,       2 ); // 100
        }
        else {
            // might not be able to reach them on the ground
            AISetSubStateChance( e_mainSubState_Open6Pool4,          2 ); //   2
            AISetSubStateChance( e_mainSubState_OpenBasicZergling4,  2 ); //   4
            AISetSubStateChance( e_mainSubState_OpenBasicRoach4,    32 ); //  46
            AISetSubStateChance( e_mainSubState_OpenRoachTech4,     64 ); //  98 
            AISetSubStateChance( e_mainSubState_OpenLingTech4,       2 ); // 100
        }
    }

    AISetMainState(player, e_mainState_OpenGnd4, AIChooseSubState());
}

//--------------------------------------------------------------------------------------------------
//  ZergOpenGndGoMid4
//--------------------------------------------------------------------------------------------------
static void ZergOpenGndGoMid4(int player) {
    int curSubState = AIState(player, e_mainSubState);

    AINewChooseSubState(player, e_middleRollState, 1, 1000, e_mainSubState_MidRoachHydra4);
    
    if (AIGetNumEnemies(player) == 1) {
        if (curSubState == e_mainSubState_Open6Pool4) {                 //  2,     4,     2,     2
            // did a very quick "6 pool" zergling rush with no gas       random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyBaneling4, 20,    12,    32,    12 );  //  20,  12,  32,  12
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyRoach4,    26,    34,    14,    32 );  //  46,  46,  46,  44
            AISetSubStateChanceRace( e_mainSubState_MidLingMidBaneling4,   20,    12,    32,    12 );  //  66,  58,  78,  56
            AISetSubStateChanceRace( e_mainSubState_MidLingMidRoach4,      26,    34,    14,    32 );  //  92,  92,  92,  88
            AISetSubStateChanceRace( e_mainSubState_MidLingMuta4,           8,     8,     8,    12 );  // 100, 100, 100, 100
        }
        else if (curSubState == e_mainSubState_OpenBasicZergling4) {    //  6,     6,     6,     2
            // did a basic zergling build with a single gas              random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyBaneling4, 12,    12,    22,    12 );  //  12,  12,  22,  12
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyRoach4,    20,    20,    10,    20 );  //  32,  32,  32,  32
            AISetSubStateChanceRace( e_mainSubState_MidLingMidBaneling4,   12,    12,    22,    12 );  //  44,  44,  54,  44
            AISetSubStateChanceRace( e_mainSubState_MidLingMidRoach4,      20,    20,    10,    20 );  //  64,  64,  64,  64
            AISetSubStateChanceRace( e_mainSubState_MidLingBaneHydra4,     14,    16,    22,    10 );  //  78,  80,  86,  74
            AISetSubStateChanceRace( e_mainSubState_MidRoachHydra4,        10,    10,    10,    10 );  //  88,  90,  96,  84
            AISetSubStateChanceRace( e_mainSubState_MidLingMuta4,          12,    10,     4,    16 );  // 100, 100, 100, 100
        }
        else if (curSubState == e_mainSubState_OpenBasicRoach4) {       // 60,    56,    38,    62
            // did a basic roach build with a single gas                 random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyRoach4,    10,     8,     6,    12 );  //  10,   8,   6,  12
            AISetSubStateChanceRace( e_mainSubState_MidLingMidRoach4,      10,     8,     6,    12 );  //  20,  16,  12,  24
            AISetSubStateChanceRace( e_mainSubState_MidRoachRush4,         48,    50,    54,    48 );  //  68,  66,  66,  72
            AISetSubStateChanceRace( e_mainSubState_MidRoachHydra4,        28,    28,    32,    22 );  //  96,  94,  98,  94
            AISetSubStateChanceRace( e_mainSubState_MidRoachMuta4,          4,     6,     2,     6 );  // 100, 100, 100, 100
        }
        else if (curSubState == e_mainSubState_OpenRoachTech4) {        // 30,    32,    52,    32
            // did a basic roach opening with double gas                 random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyRoach4,     4,     4,     2,     6 );  //   4,   4,   2,   6
            AISetSubStateChanceRace( e_mainSubState_MidLingMidRoach4,       4,     4,     2,     6 );  //   8,   8,   4,  12
            AISetSubStateChanceRace( e_mainSubState_MidRoachRush4,         18,    16,    14,    20 );  //  26,  24,  18,  32
            AISetSubStateChanceRace( e_mainSubState_MidRoachHydra4,        54,    52,    70,    44 );  //  80,  76,  88,  76
            AISetSubStateChanceRace( e_mainSubState_MidRoachMuta4,         20,    24,    12,    24 );  // 100, 100, 100, 100
        }
        else if (curSubState == e_mainSubState_OpenLingTech4) {         //  2,     2,     2,     2
            // did a basic zergling opening but with double gas          random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyBaneling4,  8,     8,     6,     8 );  //   8,   8,   6,   8
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyRoach4,     8,    12,    12,    10 );  //  16,  20,  18,  18
            AISetSubStateChanceRace( e_mainSubState_MidLingMidBaneling4,    8,     8,     6,     8 );  //  24,  28,  24,  26
            AISetSubStateChanceRace( e_mainSubState_MidLingMidRoach4,      10,    12,    12,    10 );  //  34,  40,  36,  36
            AISetSubStateChanceRace( e_mainSubState_MidLingBaneHydra4,     28,    26,    36,    18 );  //  62,  66,  72,  54
            AISetSubStateChanceRace( e_mainSubState_MidRoachHydra4,        14,     8,    18,    14 );  //  76,  74,  90,  68
            AISetSubStateChanceRace( e_mainSubState_MidLingMuta4,          24,    26,    10,    32 );  // 100, 100, 100, 100
        }
        else if (curSubState == e_mainSubState_OpenQuickExpandGnd4) {   //  0,     0,    ~6,     0
            // did a quick expand, for now only used against terran      random protoss terran zerg
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyBaneling4,  4,     4,     4,     4 );  //   4,   4,   4,   4
            AISetSubStateChanceRace( e_mainSubState_MidLingEarlyRoach4,     4,     4,     4,     4 );  //   8,   8,   8,   8
            AISetSubStateChanceRace( e_mainSubState_MidLingMidBaneling4,   22,    22,    22,    22 );  //  30,  30,  30,  30
            AISetSubStateChanceRace( e_mainSubState_MidLingMidRoach4,      14,    14,    14,    14 );  //  44,  44,  44,  44
            AISetSubStateChanceRace( e_mainSubState_MidLingBaneHydra4,     24,    24,    24,    24 );  //  68,  68,  68,  68
            AISetSubStateChanceRace( e_mainSubState_MidRoachRush4,          4,     4,     4,     4 );  //  72,  72,  72,  72
            AISetSubStateChanceRace( e_mainSubState_MidRoachHydra4,        16,    16,    16,    16 );  //  88,  88,  88,  88
            AISetSubStateChanceRace( e_mainSubState_MidRoachMuta4,          4,     4,     4,     4 );  //  92,  92,  92,  92
            AISetSubStateChanceRace( e_mainSubState_MidLingMuta4,           8,     8,     8,     8 );  // 100, 100, 100, 100
        }
    }
    else {
        if (curSubState == e_mainSubState_Open6Pool4) {  // 2
            // did a very quick "6 pool" zergling rush with no gas
            AISetSubStateChance( e_mainSubState_MidLingEarlyBaneling4,  20 );  //  20
            AISetSubStateChance( e_mainSubState_MidLingEarlyRoach4,     12 );  //  32
            AISetSubStateChance( e_mainSubState_MidLingMidBaneling4,    20 );  //  52
            AISetSubStateChance( e_mainSubState_MidLingMidRoach4,       30 );  //  82
            AISetSubStateChance( e_mainSubState_MidLingMuta4,           18 );  // 100
        }
        else if (curSubState == e_mainSubState_OpenBasicZergling4) {  // 6
            // did a basic zergling build with a single gas
            AISetSubStateChance( e_mainSubState_MidLingEarlyBaneling4,  10 );  //  10
            AISetSubStateChance( e_mainSubState_MidLingEarlyRoach4,     16 );  //  26
            AISetSubStateChance( e_mainSubState_MidLingMidBaneling4,    10 );  //  36
            AISetSubStateChance( e_mainSubState_MidLingMidRoach4,       16 );  //  52
            AISetSubStateChance( e_mainSubState_MidLingBaneHydra4,      20 );  //  72
            AISetSubStateChance( e_mainSubState_MidRoachHydra4,          6 );  //  78
            AISetSubStateChance( e_mainSubState_MidLingMuta4,           22 );  // 100
        }
        else if (curSubState == e_mainSubState_OpenBasicRoach4) {  // 56
            // did a basic roach build with a single gas
            AISetSubStateChance( e_mainSubState_MidLingEarlyRoach4,      8 );  //   8
            AISetSubStateChance( e_mainSubState_MidLingMidRoach4,        6 );  //  14
            AISetSubStateChance( e_mainSubState_MidRoachRush4,          54 );  //  68
            AISetSubStateChance( e_mainSubState_MidRoachHydra4,         28 );  //  96
            AISetSubStateChance( e_mainSubState_MidRoachMuta4,           4 );  // 100
        }
        else if (curSubState == e_mainSubState_OpenRoachTech4) {  // 34
            // did a basic roach opening with double gas to tech up
            AISetSubStateChance( e_mainSubState_MidLingEarlyRoach4,      4 );  //   4
            AISetSubStateChance( e_mainSubState_MidLingMidRoach4,        4 );  //   8
            AISetSubStateChance( e_mainSubState_MidRoachRush4,          16 );  //  24
            AISetSubStateChance( e_mainSubState_MidRoachHydra4,         50 );  //  74
            AISetSubStateChance( e_mainSubState_MidRoachMuta4,          26 );  // 100
        }
        else if (curSubState == e_mainSubState_OpenLingTech4) {  // 2
            // did a basic zergling opening but with double gas to tech up
            AISetSubStateChance( e_mainSubState_MidLingEarlyBaneling4,   6 );  //   6
            AISetSubStateChance( e_mainSubState_MidLingEarlyRoach4,      6 );  //  12
            AISetSubStateChance( e_mainSubState_MidLingMidBaneling4,    16 );  //  28
            AISetSubStateChance( e_mainSubState_MidLingMidRoach4,       20 );  //  48
            AISetSubStateChance( e_mainSubState_MidLingBaneHydra4,      32 );  //  80
