//--------------------------------------------------------------------------------------------------
//  ZergMid4
//--------------------------------------------------------------------------------------------------
void ZergMid4 (int player) {
    int mainSubState = AIState(player, e_mainSubState);
    
    if (AIGetGroundStartLocs(PlayerStartLocation(player)) > 0) {
        AIEarlyDefenseScouting(player);
    }

    if (mainSubState == e_mainSubState_MidLingEarlyBaneling4)    { ZergMidLingEarlyBaneling4(player); }
    else if (mainSubState == e_mainSubState_MidLingEarlyRoach4)  { ZergMidLingEarlyRoach4(player); }
    else if (mainSubState == e_mainSubState_MidLingMidBaneling4) { ZergMidLingMidBaneling4(player); }
    else if (mainSubState == e_mainSubState_MidLingMidRoach4)    { ZergMidLingMidRoach4(player); }
    else if (mainSubState == e_mainSubState_MidLingBaneHydra4)   { ZergMidLingBaneHydra4(player); }
    else if (mainSubState == e_mainSubState_MidRoachRush4)       { ZergMidRoachRush4(player); }
    else if (mainSubState == e_mainSubState_MidRoachHydra4)      { ZergMidRoachHydra4(player); }
    else if (mainSubState == e_mainSubState_MidRoachMuta4)       { ZergMidRoachMuta4(player); }
    else if (mainSubState == e_mainSubState_MidLingMuta4)        { ZergMidLingMuta4(player); }
    else if (mainSubState == e_mainSubState_MidAirDropRush4)     { ZergMidAirDropRush4(player); }
    else if (mainSubState == e_mainSubState_MidAirMutaRush4)     { ZergMidAirMutaRush4(player); }
    else if (mainSubState == e_mainSubState_MidAirDrop4)         { ZergMidAirDrop4(player); }
    else if (mainSubState == e_mainSubState_MidAirMuta4)         { ZergMidAirMuta4(player); }
    else if (mainSubState == e_mainSubState_MidAirMutaCorr4)     { ZergMidAirMutaCorr4(player); }
    else { ErrorMeleeScript(player, "Invalid Mid mainSubState"); }
}

