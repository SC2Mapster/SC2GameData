            AISetSubStateChance( e_mainSubState_MidLingMuta4,           16 );  // 100
        }
        else if (curSubState == e_mainSubState_OpenQuickExpandGnd4) {  // 0
            // did a quick expand, should never have been choosen with multiple opponents??
            AISetSubStateChance( e_mainSubState_MidLingMidRoach4,       10 );  //  10
            AISetSubStateChance( e_mainSubState_MidLingBaneHydra4,      10 );  //  20
            AISetSubStateChance( e_mainSubState_MidRoachHydra4,         60 );  //  80
